<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">


    <style name="MSV_ImageButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">?android:attr/selectableItemBackgroundBorderless</item>
        <item name="android:paddingLeft">@dimen/search_icon_padding</item>
        <item name="android:paddingRight">@dimen/search_icon_padding</item>
    </style>

    <style name="ExpandedAppbar" parent="@android:style/TextAppearance.Material.Title">
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">@color/colorPrimary</item>
    </style>

    <style name="RatingDialogStyle" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:background">#FFFFFF</item>
        <item name="android:buttonBarPositiveButtonStyle" tools:targetApi="lollipop">
            @style/RatingDialogButtonStyle
        </item>
        <item name="android:buttonBarNegativeButtonStyle" tools:targetApi="lollipop">
            @style/RatingDialogButtonStyle
        </item>

    </style>


</resources>